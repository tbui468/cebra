
images_f := open("./../../examples/neural_network/t10k-images.idx3-ubyte")
image_data := read_bytes(images_f)
close(images_f)

f := open("./../../examples/neural_network/t10k-labels.idx1-ubyte")
bytes := read_bytes(f)
close(f)


for i := 0, i < 16, i = i + 1 {
  print(image_data[i]) print("\n")
}

counts := List<int>()
for i := 0, i < 10, i = i + 1 {
  counts = counts ++ 0
}

for i := 8, i < bytes.size, i = i + 1 {
  idx := bytes[i]
  counts[idx] = counts[idx] + 1
}

for i := 0, i < 10, i = i + 1 {
  print(i as string + ": " + counts[i] as string + "\n")
}

print("bytes: " + bytes.size as string + "\n")
