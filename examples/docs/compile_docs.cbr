dir := "C:\\dev\\cebra\\examples\\docs\\"

f := open(dir + "cebra_docs.html")

out := open(dir + "compiled_docs.html")
clear(out)

while !eof(f) {
    s := read_line(f)
    if s.size > 0 and s[0] == "$"{
        about_f := open(dir + s[1, s.size])
        about_s := read_all(about_f)
        append(out, about_s)
        close(about_f)
    } else {
        append(out, s)
    } 
}

close(f)
close(out)
